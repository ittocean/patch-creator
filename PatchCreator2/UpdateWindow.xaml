        <Label x:Name="contentLabel" Content="&lt;&lt;Label&gt;&gt;" Margin="10,30,10,0" VerticalAlignment="Top" Height="73" Foreground="White"/>
        <Ellipse x:Name="ellipseButton" Height="58" Margin="0,0,0,10" VerticalAlignment="Bottom" Stroke="Black" Cursor="Hand" StrokeThickness="2" MouseLeftButtonUp="ellipseButton_MouseLeftButtonUp" MouseEnter="ellipseButton_MouseEnter" MouseLeave="ellipseButton_MouseLeave" ToolTip="Create Patch" Focusable="True" Width="58">
